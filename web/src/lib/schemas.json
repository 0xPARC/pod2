            "required": ["Dictionary"],
            "type": "object"
          },
          {
            "additionalProperties": false,
            "properties": {
              "Int": {
                "format": "int64",
                "type": "integer"
              }
            },
            "required": ["Int"],
            "type": "object"
          },
          {
            "additionalProperties": false,
            "properties": {
              "Raw": {
